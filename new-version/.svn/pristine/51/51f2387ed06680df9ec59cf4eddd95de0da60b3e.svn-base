				fnLess($(this), '.num', '.less-disable', 500, 100);
			});

			$('.tenderAmount .plus').bind('click', function(){
				fnPlus($(this), '.num', '.less', '.less-disable', 500, 100);
			});

			$('.tenderAmount .num').each(function(){
				var _this=$(this);
				var nValue=parseInt(_this.text());
				if(nValue<=500){
					_this.parentsUntil('numBox').parent().find('.less').addClass('dis_none');
					_this.parentsUntil('numBox').parent().find('.less-disable').removeClass('dis_none');
				}else{
					_this.parentsUntil('numBox').parent().find('.less').removeClass('dis_none');
