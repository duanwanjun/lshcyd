/*弹窗*/
.popup_bg{ width:100%; height:100%; position:fixed; top:0px; left:0px; z-index:999998; _position:absolute; _height:2000px;background:#000; filter: alpha(opacity=30); -moz-opacity: .30; opacity: 0.3;}
