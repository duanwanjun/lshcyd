.dialog .tip_information .successful{ background-position:0 -105px;}
.dialog .tip_information .question{background-position:0 -138px;}
.dialog .tip_information .tips{display:inline-block; text-align:left; margin-left:10px; vertical-align:top; max-width:410px;}
